{
  "name": "dominickolbe",
  "version": "2.3.1",
  "license": "MIT",
  "author": "Dominic Kolbe <kolbedominic@gmail.com>",
  "repository": "git@github.com:dominickolbe/dominickolbe.git",
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version REACT_APP_BUILD_TIME=$(date '+%x %X') react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version REACT_APP_BUILD_TIME=$(date '+%x %X') react-scripts build",
    "test": "yarn build",
    "prettier:check": "prettier --check src/**/*.{ts,tsx,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,tsx,json}"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "prettier": "^2.3.0",
    "typescript": "~4.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.22.x"
  }
}
