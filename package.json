  "repository": "git@github.com:dominickolbe/dominickolbe.git",
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version REACT_APP_BUILD_TIME=$(date '+%c') react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version REACT_APP_BUILD_TIME=$(date '+%c') BUILD_PATH=./dist react-scripts build",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,html}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,html}\"",
    "test": "pnpm run prettier:check"
  },
  "dependencies": {
    "@emotion/css": "11.11.2",
    "@vercel/analytics": "1.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "prettier": "3.2.5",
    "typescript": "5.4.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
