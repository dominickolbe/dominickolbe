{
  "name": "dominickolbe",
  "version": "2.5.1",
  "license": "MIT",
  "author": "Dominic Kolbe <kolbedominic@gmail.com>",
  "repository": "git@github.com:dominickolbe/dominickolbe.git",
  "scripts": {
    "start": "REACT_APP_VERSION=$npm_package_version REACT_APP_BUILD_TIME=$(date '+%x %X') react-scripts start",
    "build": "REACT_APP_VERSION=$npm_package_version REACT_APP_BUILD_TIME=$(date '+%x %X') react-scripts build",
    "prettier:check": "prettier --check src/**/*.{ts,tsx,json}",
    "prettier:fix": "prettier --write src/**/*.{ts,tsx,json}",
    "test": "yarn run prettier:check"
  },
  "dependencies": {
    "@emotion/css": "11.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@types/node": "16.6.1",
    "@types/react": "17.0.17",
    "@types/react-dom": "17.0.9",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
